<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="#ffefeff4" />
            <corners android:topRightRadius="12.0dip" android:bottomRightRadius="12.0dip" />
            <stroke android:width="1.0dip" android:color="#ffe5e5ea" />
        </shape>
    </item>
    <item android:state_selected="false">
        <shape>
            <solid android:color="@color/white" />
            <corners android:topRightRadius="12.0dip" android:bottomRightRadius="12.0dip" />
            <stroke android:width="1.0dip" android:color="#ffe5e5ea" />
        </shape>
    </item>
</selector>